
<h1>Listing storeinfos</h1>

<table>
  <thead>
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Address</th>
      <th>Name</th>
      <th>Url</th>
      <th>Sun time</th>
      <th>Mon time</th>
      <th>Tue time</th>
      <th>Web time</th>
      <th>Thu time</th>
      <th>Fri time</th>
      <th>Sta time</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @storeinfos.each do |storeinfo| %>
    <tr>
      <td><%= storeinfo.latitude %></td>
      <td><%= storeinfo.longitude %></td>
      <td><%= storeinfo.address %></td>
      <td><%= storeinfo.name %></td>
      <td><%= storeinfo.url %></td>
      <td><%= storeinfo.sun_time %></td>
      <td><%= storeinfo.mon_time %></td>
      <td><%= storeinfo.tue_time %></td>
      <td><%= storeinfo.web_time %></td>
      <td><%= storeinfo.thu_time %></td>
      <td><%= storeinfo.fri_time %></td>
      <td><%= storeinfo.sta_time %></td>
      <td><%= link_to 'Show', storeinfo %></td>
      <td><%= link_to 'Edit', edit_storeinfo_path(storeinfo) %></td>
      <td><%= link_to 'Destroy', storeinfo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'New Storeinfo', new_storeinfo_path %>


